   "source": [
    "dan = 1\n",
    "\n",
    "while dan :\n",
    "    dan = int(input('몇단을 출력할까요?(종료: 0) : '))\n",
    "    if dan == 0 :\n",
    "        break\n",
    "    else:\n",
    "        for i in range (1, 10) :\n",
    "            print('{} * {} = {}'.format(dan,i,dan*i))"
